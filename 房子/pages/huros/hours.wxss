/* pages/mycenter/mycenter.wxss */
page{
  width: 100%;
  height: 100%;
}

.popup-backdrop
{
	position: fixed;
    z-index: 10000;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    transition-duration: 400ms;
    background: rgba(0,0,0,.4);
    width: 100%;
    height: 100%;
}

.item{
  display: flex;justify-content: center;align-items: center;
}

.item1{
  display: flex;justify-content: center;
}

.item2{
  display: flex;align-items: center;
}

.but{
  width:65%;height:80%;border-radius:30rpx;font-size:30rpx;color:#fff;background-color:rgb(241,112,59);
}
.view2{
  width:100%;height:50%;
}
.view1{
  width:30%;height:62%;font-size:24rpx;border-right:1rpx solid #fff
}